	journal = {Journal of Chemical Theory and Computation},
	author = {Gaus, Michael and Lu, Xiya and Elstner, Marcus and Cui, Qiang},
	month = apr,
	year = {2014},
	pages = {1518--1537},
	file = {Gaus et al. - 2014 - Parameterization of DFTB33OB for Sulfur and Phosp.pdf:/Users/vviterbo/Zotero/storage/JM74B49H/Gaus et al. - 2014 - Parameterization of DFTB33OB for Sulfur and Phosp.pdf:application/pdf}
}

