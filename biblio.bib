	pages = {10280--10287},
	file = {Liu et al. - 2013 - Hollow, Spherical Nitrogen-Rich Porous Carbon Shel.pdf:/Users/vviterbo/Zotero/storage/54YAIXE6/Liu et al. - 2013 - Hollow, Spherical Nitrogen-Rich Porous Carbon Shel.pdf:application/pdf}
